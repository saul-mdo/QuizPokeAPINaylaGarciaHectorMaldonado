<UserControl x:Class="QuizPokemonWPF.Views.InicioUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuizPokemonWPF.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel >
        <DockPanel.Background>
            <ImageBrush ImageSource="/images/fondo.jpg" ></ImageBrush>
        </DockPanel.Background>
        <Image DockPanel.Dock="Top" Source="/images/pokequiz.png" Height="150px"></Image>
        <Button DockPanel.Dock="Left" Width="250" Height="80" Margin="100,100,50,100" Background="Transparent" BorderThickness="0">
            <Image Source="" ></Image>
        </Button>
        <Image Source="" Width="400"></Image>
    </DockPanel>
</UserControl>
